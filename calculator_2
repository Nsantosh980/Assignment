print("1.+")
print("2.-")
print("3.*")
print("4./")
operator = int(input("select operator(1-4):"))
num1=float(input("Enter num1:"))
num2=float(input("Enter num2:"))
if operator==1:
    print("sum = ",num1+num2)
elif operator==2:
    print("sub = ",num1-num2)
elif operator==3:
    print("multiple = ",num1*num2)
elif operator==4:
    print("divide = ",num1/num2)
else:
    print("Invalid option!!")

